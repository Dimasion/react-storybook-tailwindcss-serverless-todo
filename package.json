{
  "name": "react-todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run build:tw && react-scripts build",
    "start:storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "start:tw": "postcss ./src/assets/css/tailwind.src.css -o ./src/assets/css/tailwind.css -w",
    "build:tw": "postcss ./src/assets/css/tailwind.src.css -o ./src/assets/css/tailwind.css --env production",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.0.11",
    "@storybook/addon-storyshots": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "clsx": "^1.0.4",
    "node-sass": "^4.12.0",
    "postcss-cli": "^6.1.2",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4",
    "tailwindcss": "^1.0.1",
    "tailwindcss-transition": "^1.0.5"
  }
}
